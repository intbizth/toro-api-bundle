{
    "name": "toro/api-bundle",
    "type": "symfony-bundle",
    "description": "APIs management for Symfony2 projects.",
    "keywords": ["api", "rest"],
    "homepage": "https://github.com/intbizth/toro-api-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "liverbool",
            "homepage": "https://github.com/liverbool"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "friendsofsymfony/oauth-server-bundle": "~1.4",
        "symfony/framework-bundle": "^2.8",
        "symfony/form": "^2.8",
        "symfony/validator": "^2.8"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "phpspec/phpspec": "^3.1",
        "phpunit/phpunit": "^5.6",
        "twig/twig": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Toro\\Bundle\\ApiBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Toro\\Bundle\\ApiBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
